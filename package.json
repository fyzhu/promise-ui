{
  "name": "promise-ui",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "register:components": "vitepress-rc",
    "cli:dev": "esbuild --bundle ./promiseui-cli/index.js --format=cjs --platform=node --outdir=./lib --watch",
    "cli:build": "esbuild --bundle ./promiseui-cli/index.js --format=cjs --platform=node --outdir=./lib",
    "cli": "node ./lib/index.js create"
  },
  "dependencies": {
    "vue": "^3.2.25"
  },
  "devDependencies": {
    "@types/node": "^17.0.32",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "typescript": "^4.5.4",
    "vite": "^2.9.9",
    "vitepress": "^0.22.4",
    "vitepress-theme-demoblock": "^1.4.2",
    "vue-tsc": "^0.34.7"
  }
}
